	<weapon id="47927" name="Magic Fan - Blue" type="ROD" body-part="RIGHT_HAND" magic="true">
	<!-- Within the swift breezes you can feel a magic energy. This magic fan is given out to commemorate the 13th anniversary of Lineage 2. -->
		<restriction tradable="false" dropable="false" sellable="false" private-sellable="false"/>
		<attributes weight="150" commission-type="OTHER_ITEM" enchant-enabled="false"/>
		<crystal />
		<damage radius="40" angle="240"/>
		<stats>
			<stat type="ACCURACY" value="8.0"/>
			<stat type="CRITICAL_RATE" value="6.0"/>
			<stat type="MAGIC_ATTACK" value="1.0"/>
			<stat type="PHYSICAL_ATTACK" value="1.0"/>
			<stat type="PHYSICAL_ATTACK_RANGE" value="40.0"/>
			<stat type="PHYSICAL_ATTACK_SPEED" value="379.0"/>
			<stat type="RANDOM_DAMAGE" value="1.0"/>
		</stats>
	</weapon>

	<item id="47928" name="Bundle of Magical Firewood" type="NONE">
	<!-- Double-click to use a Magic Woodpile for one player. If you have the Magic Fan equipped, you can use magic to set fire to the woodpile. -->
		<restriction/>
		<attributes action="SKILL_REDUCE"/>
	</item>

	<item id="47929" name="Big Bundle of Magical Firewood" type="NONE">
	<!-- Double-click to use a Large Magic Woodpile for party use. If you have the Magic Fan equipped, you can use magic to set fire to the woodpile. -->
		<restriction/>
		<attributes action="SKILL_REDUCE"/>
	</item>

