	<item id="5703" name="Lucky Charm (No-grade)" type="SCROLL">
	<!-- estows blessing on its owner such that when the owner dies during a boss/raid war, their abilities will not be diminished. This item can only be used on self by a character level 19 or below. -->
		<restriction olympiad-restricted="true"/>
		<attributes weight="10" price="100" reuse-delay="10000" action="SKILL_REDUCE_ON_SKILL_SUCCESS"/>
		<skill-reducer type="ItemSkills">
			<skill id="2168" value="-1"/>  <!-- Lucky Charm -->
		</skill-reducer>
	</item>

