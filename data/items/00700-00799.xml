	<item id="729" name="Scroll: Enchant A-grade Weapon (Exchangeable)" type="ENCHANT_WEAPON">
	<!-- <+1~3 Weapon Enchant Effect>\nOne-handed Swords /One-handed Blunt Weapon / Dagger / Rapier / Spear: P. Atk. +10, M. Atk. +6\nTwo-handed Blunt / Dualsword / Fist Weapon / Ancient Sword: P. Atk. +12, M. Atk. +6\nBow/Shooter: P. Atk. +16, M. Atk. +6\n\n<+4 or higher Weapon Enchant Effect>\nOne-handed Swords /One-handed Blunt Weapon / Dagger / Rapier / Spear: P. Atk. +30, M. Atk. +18\nTwo-handed Blunt / Dualsword / Fist Weapon / Ancient Sword: P. Atk. +36, M. Atk. +18\nBow/Shooter: P. Atk. +48, M. Atk. +18\n\n<Safe Enchant Level: +4> -->
		<restriction olympiad-restricted="true"/>
		<attributes price="1800000" commission-type="SCROLL_ENCHANT_WEAPON" immediate-effect="true" action="SKILL_REDUCE"/>
		<random_craft exp="900000" commission="18000"/>
		<action handler="EnchantScrolls"/>
	</item>

	<item id="730" name="Scroll: Enchant A-grade Armor (Exchangeable)" type="ENCHANT_ARMOR">
