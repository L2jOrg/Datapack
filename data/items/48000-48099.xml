	<item id="48046" name="Agathion Enchant Scroll" type="ENCHANT_AGATHION">
	<!-- Only for Zodiac Agathion. Increases M. Def. when the Agathion enchant level increases. Can be safely enchanted up to +3 and can be enchanted up to +10. Starting from +1 enchanting special abilities increases step by step. Item is destroyed upon enchant failure. -->
		<restriction/>
		<attributes action="SKILL_REDUCE"/>
		<action handler="EnchantScrolls"/>
	</item>

	<item id="48047" name="High-grade Agathion Enchant Scroll" type="MULTI_ENCHANT_AGATHION">
	<!-- Only for Zodiac Agathion. Increases M. Def. when the Agathion enchant level increases. Can be safely enchanted up to +3. Starting from +1 enchanting special abilities increases step by step. Enchant randomly from +1 to +3 on success. Item is destroyed upon enchant failure. Cannot use for items with enchant level more than +8. -->
		<restriction/>
		<attributes action="SKILL_REDUCE"/>
		<action handler="EnchantScrolls"/>
	</item>

