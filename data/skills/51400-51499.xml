                <power initial="63"/>
            </stat-modify>
            <stat-modify stat="MAGIC_ATTACK">
                <power initial="70"/>
            </stat-modify>
            <trigger-skill-by-attack allow-reflect="false" critical-only="false" skill="51460" target="MY_PARTY" trigger-by-normal-attack="true" trigger-by-skill="true">
                <power initial="1"/>
                <chance initial="40"/>
            </trigger-skill-by-attack>
        </effects>
    </skill>
