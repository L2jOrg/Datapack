    <skill id="55309" name="Earring of Chronicle Heir" max-level="10">
        <!-- 
			level 1: MP Recovery Rate +1%, Max MP +1%.
			level 2: MP Recovery Rate +2%, Max MP +2%.
			level 3: MP Recovery Rate +3%, Max MP +3%.
			level 4: MP Recovery Rate +4%, Max MP +4%.
			level 5: MP Recovery Rate +5%, Max MP +5%.
			level 6: MP Recovery Rate +6%, Max MP +6%.
			level 7: MP Recovery Rate +7%, Max MP +7%.
			level 8: MP Recovery Rate +8%, Max MP +8%.
			level 9: MP Recovery Rate +9%, Max MP +9%.
			level 10: MP Recovery Rate +10%, Max MP +10%.
		-->
        <icon initial="icon.event_classic_ring"/>
        <attributes/>
        <target/>
        <effects>
            <stat-modify mode="PER" stat="REGENERATE_MP_RATE">
                <power initial="1.0">
                    <value level="2">2.0</value>
                    <value level="3">3.0</value>
                    <value level="4">4.0</value>
                    <value level="5">5.0</value>
                    <value level="6">6.0</value>
                    <value level="7">7.0</value>
                    <value level="8">8.0</value>
                    <value level="9">9.0</value>
                    <value level="10">10.0</value>
                </power>
            </stat-modify>
            <vital-stat-modify mode="PER" stat="MAX_MP" stat-add="MAX_MP">
                <power initial="1.0">
                    <value level="2">2.0</value>
                    <value level="3">3.0</value>
                    <value level="4">4.0</value>
                    <value level="5">5.0</value>
                    <value level="6">6.0</value>
                    <value level="7">7.0</value>
                    <value level="8">8.0</value>
                    <value level="9">9.0</value>
                    <value level="10">10.0</value>
                </power>
            </vital-stat-modify>
        </effects>
    </skill>
