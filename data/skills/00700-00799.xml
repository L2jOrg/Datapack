            </vital-stat-modify>
            <stat-modify stat="HEAL_RECEIVE">
                <power initial="-30.0"/>
            </stat-modify>
            <resist-abnormal-by-category category="DEBUFF">
                <power initial="-30.0"/>
            </resist-abnormal-by-category>
        </effects>
    </skill>
