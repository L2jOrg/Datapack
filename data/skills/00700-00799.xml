            <level initial="1"/>
            <time initial="60"/>
            <chance initial="80"/>
        </abnormal>
        <conditions>
            <weapon>
                <type>SWORD TWO_HAND_SWORD MAGIC_SWORD BLUNT HAMMER ROD STAFF</type>
            </weapon>
        </conditions>
        <effects>
            <physical-attack critical-chance="50.0" ignore-shield="true" over-hit="false">
                <power initial="8409.0"/>
            </physical-attack>
            <dispel-by-category category="BUFF" max="3">
