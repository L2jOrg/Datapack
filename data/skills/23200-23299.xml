<?xml version="1.0" encoding="UTF-8"?>
<list xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://l2j.org" xsi:schemaLocation="http://l2j.org skills.xsd">
    <skill id="23234" name="Singer &amp; Dancer Agathion Cute Trick" action="A1">
        <!-- See the summoned spirit perform cute tricks. -->
        <icon initial="icon.skill_agathion_cute"/>
        <attributes magic-critical-rate="-5.0">
            <magic-level initial="85"/>
            <hit-time initial="3000"/>
        </attributes>
        <target/>
        <conditions>
            <condition name="OpNeedAgathion"/>
        </conditions>
    </skill>
    <skill id="23298" name="Flame Hawk" debuff="true" action="A2">
        <!-- For 10 seconds, becomes engulfed in flames to receive 250 damage per second. -->
        <icon initial="icon.skill0771"/>
        <attributes property="PHYSIC" magic-critical-rate="-5.0" level-bonus-rate="1" triggered="true">
            <magic-level initial="80"/>
            <effect-point initial="-800"/>
            <element type="FIRE" value="20"/>
        </attributes>
        <target/>
        <abnormal type="DOT_ATTR">
            <level initial="1"/>
            <time initial="10"/>
            <visual initial="DOT_BLEEDING"/>
        </abnormal>
        <effects>
            <damage-over-time ticks="5">
                <power initial="250.0"/>
            </damage-over-time>
        </effects>
    </skill>
    <skill id="23299" name="Arrow Rain" debuff="true" action="A2">
        <!-- For 10 seconds, becomes swept into a gust of wind to receive 250 damage per second. -->
        <icon initial="icon.skill0772"/>
        <attributes property="PHYSIC" magic-critical-rate="-5.0" level-bonus-rate="1" triggered="true">
            <magic-level initial="80"/>
            <effect-point initial="-800"/>
            <element type="WIND" value="20"/>
        </attributes>
        <target type="TARGET"/>
        <abnormal type="DOT_ATTR">
            <level initial="1"/>
            <time initial="10"/>
            <visual initial="DOT_BLEEDING"/>
        </abnormal>
        <effects>
            <damage-over-time ticks="5">
                <power initial="250.0"/>
            </damage-over-time>
        </effects>
    </skill>
</list>