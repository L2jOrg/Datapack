    <skill id="19009" name="Clan Advent" max-level="10" action="A2">
        <!--
            level  1: When the clan leader logs in, the following effects are applied.\n\nP. Atk. +5\nM. Atk. +5.
            level  2: When the clan leader logs in, the following effects are applied.\n\nP. Atk. +10\nM. Atk. +10.
            level  3: When the clan leader logs in, the following effects are applied.\n\nP. Atk. +15\nM. Atk. +15.
            level  4: When the clan leader logs in, the following effects are applied.\n\nP. Atk. +25\nM. Atk. +25.
            level  5: When the clan leader logs in, the following effects are applied.\n\nP. Atk. +50\nM. Atk. +50.
            level  6: When the clan leader logs in, the following effects are applied.\n\nP. Atk. +100\nM. Atk. +100.
            level  7: When the clan leader logs in, the following effects are applied.\n\nP. Atk. +200\nM. Atk. +200.
            level  8: When the clan leader logs in, the following effects are applied.\n\nP. Atk. +200\nM. Atk. +200\nP. Def. +50\nM. Def. +50.
            level  9: When the clan leader logs in, the following effects are applied.\n\nP. Atk. +200\nM. Atk. +200\nP. Def. +100\nM. Def. +100.
            level 10: When the clan leader logs in, the following effects are applied.\n\nP. Atk. +200\nM. Atk. +200\nP. Def. +200\nM. Def. +200.
        -->
        <icon initial="icon.skill19009" />
        <attributes property="PHYSIC" magic-critical-rate="5.0" stay-after-death="true" dispellable="false" irreplacable="true">
            <magic-level initial="-2" />
            <effect-point initial="100" />
        </attributes>
        <target scope="PLEDGE" object="FRIEND" range="1000" affect-min="50" affect-random="50" />
        <abnormal type="CLAN_ADVENT">
            <level initial="1" />
            <time initial="-1" />
        </abnormal>
        <effects>
            <stat-modify mode="PER" stat="PHYSICAL_ATTACK">
                <power initial="5">
                    <value level="2">10</value>
                    <value level="3">15</value>
                    <value level="4">25</value>
                    <value level="5">50</value>
                    <value level="6">100</value>
                    <value level="7">200</value>
                    <value level="8">200</value>
                    <value level="9">200</value>
                    <value level="10">200</value>
                </power>
            </stat-modify>
            <stat-modify mode="PER" stat="MAGIC_ATTACK">
                <power initial="5">
                    <value level="2">10</value>
                    <value level="3">15</value>
                    <value level="4">25</value>
                    <value level="5">50</value>
                    <value level="6">100</value>
                    <value level="7">200</value>
                    <value level="8">200</value>
                    <value level="9">200</value>
                    <value level="10">200</value>
                </power>
            </stat-modify>
            <stat-modify mode="PER" stat="PHYSICAL_DEFENCE">
                <power initial="0">
                    <value level="2">0</value>
                    <value level="3">0</value>
                    <value level="4">0</value>
                    <value level="5">0</value>
                    <value level="6">0</value>
                    <value level="7">0</value>
                    <value level="8">50</value>
                    <value level="9">100</value>
                    <value level="10">200</value>
                </power>
            </stat-modify>
            <stat-modify mode="PER" stat="MAGICAL_DEFENCE">
                <power initial="0">
                    <value level="2">0</value>
                    <value level="3">0</value>
                    <value level="4">0</value>
                    <value level="5">0</value>
                    <value level="6">0</value>
                    <value level="7">0</value>
                    <value level="8">50</value>
                    <value level="9">100</value>
                    <value level="10">200</value>
                </power>
            </stat-modify>
        </effects>
    </skill>
