    <skill id="17823" name="Aquamarine" max-level="10">
        <!-- 
			level 1: With a 10% chance, recovers 12 MP when using skills.
			level 2: With a 10% chance, recovers 16 MP when using skills.
			level 3: With a 10% chance, recovers 20 MP when using skills.
			level 4: With a 10% chance, recovers 30 MP when using skills.
			level 5: With a 10% chance, recovers 50 MP when using skills.
			level 6: When using a skill there is a 10% chance of recovering 60 MP, Max MP +2%.
			level 7: When using a skill there is a 10% chance of recovering 80 MP, Max MP +5%.
			level 8: When using a skill there is a 10% chance of recovering 100 MP, Max MP +8%.
			level 9: When using a skill there is a 10% chance of recovering 130 MP, Max MP +12%.
			level 10: When using a skill there is a 10% chance of recovering 200 MP, Max MP +20%. When using a special skill recovers 10% MP.
		-->
        <icon initial="icon.etc_bm_jewel_aquamarine_i00">
            <value level="6">icon.bm_jewel_castmpup_high_lv1_lavianrose</value>
        </icon>
        <attributes/>
        <target/>
        <effects>
            <trigger-skill-by-magic skill="17824" target="SELF">
                <power initial="1.0">
                    <value level="2">2.0</value>
                    <value level="3">3.0</value>
                    <value level="4">4.0</value>
                    <value level="5">5.0</value>
                    <value level="6">6.0</value>
                    <value level="7">7.0</value>
                    <value level="8">8.0</value>
                    <value level="9">9.0</value>
                    <value level="10">10.0</value>
                </power>
                <chance initial="10.0"/>
                <types>PHYSIC MAGIC STATIC DANCE</types>
            </trigger-skill-by-magic>
        </effects>
    </skill>
