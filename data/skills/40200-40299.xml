    <skill id="40202" name="Special Dragon's Fruit Lv. 2 - Event" action="A2">
        <!-- For 20 minutes, XP/SP Gain +15%%, 
		P. Atk./M. Atk. +15%%, 
		P. Critical/M. Critical Rate +20, P. Critical Damage/M. Critical Damage +7%%, 
		P. Def./M. Def. +20%%, 
		HP/MP/CP +30%%, HP Recovery Bonus/MP Recovery Bonus +40%%, 
		and has a chance to absorb 7%% of physical damage dealt to enemies as HP.\nDeath protection. You can delete it yourself. -->
        <icon initial="icon.bm_dragon_fruit"/>
		<attributes property="PHYSIC" magic-critical-rate="5.0" stay-after-death="true" auto-use="BUFF">
			<magic-level initial="1"/>
			<reuse initial="60000"/>
			<effect-point initial="1"/>
		</attributes>
        <target/>
		<abnormal type="EXP_SP_K">
			<level initial="3"/>
			<time initial="1200"/>
		</abnormal>
		<effects>
			<stat-modify mode="PER" stat="BONUS_EXP">
				<power initial="15.0"/>
			</stat-modify>
			<stat-modify mode="PER" stat="BONUS_SP">
				<power initial="15.0"/>
			</stat-modify>
			<stat-modify mode="PER" stat="PHYSICAL_ATTACK">
				<power initial="15.0"/>
			</stat-modify>
			<stat-modify mode="PER" stat="MAGIC_ATTACK">
				<power initial="15.0"/>
			</stat-modify>
            <stat-modify mode="PER" stat="MAGICAL_DEFENCE">
                <power initial="20.0"/>
            </stat-modify>
            <stat-modify mode="PER" stat="PHYSICAL_DEFENCE">
                <power initial="20.0"/>
            </stat-modify>		
			<stat-modify stat="CRITICAL_RATE">
				<power initial="20.0"/>
			</stat-modify>
            <stat-modify stat="MAGIC_CRITICAL_RATE">
                <power initial="20.0"/>
            </stat-modify>		
           <stat-modify mode="PER" stat="CRITICAL_DAMAGE" stat-add="CRITICAL_DAMAGE_ADD">
                <power initial="7.0"/>
            </stat-modify>
            <stat-modify mode="PER" stat="MAGIC_CRITICAL_DAMAGE" stat-add="MAGIC_CRITICAL_DAMAGE_ADD">
                <power initial="7.0"/>
            </stat-modify>
			<vital-stat-modify mode="PER" stat="MAX_HP">
				<power initial="30.0"/>
			</vital-stat-modify>
			<vital-stat-modify mode="PER" stat="MAX_MP">
				<power initial="30.0"/>
			</vital-stat-modify>
			<vital-stat-modify mode="PER" stat="MAX_CP">
				<power initial="30.0"/>
			</vital-stat-modify>
			<stat-modify mode="PER" stat="REGENERATE_HP_RATE">
				<power initial="40.0"/>
			</stat-modify>
			<stat-modify mode="PER" stat="REGENERATE_MP_RATE">
				<power initial="40.0"/>
			</stat-modify>
			<vampiric-attack chance="80">
				<power initial="7.0"/>
			</vampiric-attack>
		</effects>		
