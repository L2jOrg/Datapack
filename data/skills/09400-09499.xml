<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<list xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://l2j.org" xsi:schemaLocation="http://l2j.org skills.xsd">
    <skill id="9487" name="Angel Cat's Blessing" action="A2">
        <!-- For 60 min., Max HP/ MP +5%%, acquired XP/ SP +20%%. The effect remains after death. -->
        <icon initial="icon.skilltransform4"/>
        <attributes irreplacable="true" magic-critical-rate="5.0" stay-after-death="true">
            <magic-level initial="-3"/>
            <cast-range initial="-1"/>
            <effect-point initial="1"/>
        </attributes>
        <target/>
        <abnormal type="EXP_SP_G">
            <level initial="1"/>
            <time initial="3600"/>
        </abnormal>
        <effects>
            <vital-stat-modify mode="PER" stat="MAX_HP">
                <power initial="5.0"/>
            </vital-stat-modify>
            <vital-stat-modify mode="PER" stat="MAX_MP">
                <power initial="5.0"/>
            </vital-stat-modify>
            <stat-modify mode="PER" stat="BONUS_EXP">
                <power initial="20.0"/>
            </stat-modify>
            <stat-modify mode="PER" stat="BONUS_SP">
                <power initial="20.0"/>
            </stat-modify>
        </effects>
    </skill>
</list>
