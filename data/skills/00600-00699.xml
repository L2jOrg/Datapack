<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<list xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://l2j.org" xsi:schemaLocation="http://l2j.org skills.xsd">
    <skill id="619" name="Cancel Transformation" max-level="2" action="A1">
        <!-- Cancels transformation. -->
        <icon initial="icon.skilltransform2"/>
        <attributes magic-critical-rate="5.0">
            <magic-level initial="56"/>
            <cast-range initial="-1"/>
            <effect-point initial="10"/>
            <hit-time initial="2500"/>
        </attributes>
        <target/>
        <conditions>
            <condition name="CanUntransform"/>
        </conditions>
        <effects>
            <dispel>
                <abnormal type="TRANSFORM">
                    <power initial="-1.0"/>
                </abnormal>
            </dispel>
        </effects>
    </skill>
    <skill id="622" name="Ultimate Escape" max-level="2" action="A2">
        <!-- 
			level 1: For 30 sec. P. Evasion +20 M. Evasion +20 P. Skill Evasion +20% M. Skill Evasion +20% Speed +30
			level 2: For 30 sec. P. Evasion +35 M. Evasion +35 P. Skill Evasion +30% M. Skill Evasion +30% Speed +40
		-->
        <icon initial="icon.skill0622"/>
        <attributes auto-use="BUFF" magic-critical-rate="5.0">
            <magic-level initial="28">
                <value level="2">52</value>
            </magic-level>
            <cast-range initial="-1"/>
            <reuse initial="900000"/>
            <effect-point initial="200">
                <value level="2">300</value>
            </effect-point>
            <hit-time initial="1500"/>
        </attributes>
        <consume>
            <mana initial="40">
                <value level="2">60</value>
            </mana>
        </consume>
        <target/>
        <abnormal type="AVOID_UP_SPECIAL">
            <level initial="1"/>
            <time initial="30"/>
        </abnormal>
        <effects>
            <stat-modify stat="EVASION_RATE" stat-add="EVASION_RATE">
                <power initial="20.0">
                    <value level="2">35.0</value>
                </power>
            </stat-modify>
            <stat-modify stat="MAGIC_EVASION_RATE" stat-add="MAGIC_EVASION_RATE">
                <power initial="20.0">
                    <value level="2">35.0</value>
                </power>
            </stat-modify>
            <skill-evasion type="PHYSIC">
                <power initial="20.0">
                    <value level="2">20.0</value>
                </power>
            </skill-evasion>
            <skill-evasion type="MAGIC">
                <power initial="20.0">
                    <value level="2">20.0</value>
                </power>
            </skill-evasion>
            <speed stop-level="1">
                <power initial="30"/>
            </speed>
            <speed mode="PER" start-level="2">
                <power initial="30"/>
            </speed>
        </effects>
    </skill>
    <skill id="626" name="Critical Sense" max-level="5">
        <!-- 
			level 1: P. Critical Rate +10% P. Critical Damage +100
			level 2: P. Critical Rate +12% P. Critical Damage +200
			level 3: P. Critical Rate +14% P. Critical Damage +300
			level 4: P. Critical Rate +16% P. Critical Damage +400
			level 5: P. Critical Rate +20% P. Critical Damage +500
		-->
        <icon initial="icon.skill10812"/>
        <attributes magic-critical-rate="5.0"/>
        <target/>
        <effects>
            <stat-modify stat="CRITICAL_DAMAGE" stat-add="CRITICAL_DAMAGE_ADD">
                <power initial="100.0">
                    <value level="2">200.0</value>
                    <value level="3">300.0</value>
                    <value level="4">400.0</value>
                    <value level="5">500.0</value>
                </power>
            </stat-modify>
            <stat-modify mode="PER" stat="CRITICAL_RATE">
                <power initial="10">
                    <value level="2">12</value>
                    <value level="3">14</value>
                    <value level="4">16</value>
                    <value level="5">20</value>
                </power>
            </stat-modify>
        </effects>
    </skill>
    <skill id="627" name="Mental Panic" max-level="5" action="A2" debuff="true">
        <!-- <Fear Skill>\n\nThe effect lasts for 3 sec.\nFrightens the enemy. -->
        <icon initial="icon.skill10771"/>
        <attributes auto-use="ACTIVE" level-bonus-rate="20" magic-critical-rate="5.0" trait="DERANGEMENT">
            <magic-level initial="76">
                <value level="2">78</value>
                <value level="3">80</value>
                <value level="4">82</value>
                <value level="5">84</value>
            </magic-level>
            <cast-range initial="700"/>
            <reuse initial="20000"/>
            <cool-time initial="300"/>
            <effect-point initial="-460">
                <value level="2">-470</value>
                <value level="3">-480</value>
                <value level="4">-490</value>
                <value level="5">-500</value>
            </effect-point>
            <effect-range initial="600"/>
            <hit-time initial="1500"/>
        </attributes>
        <consume>
            <mana initial="40">
                <value level="2">45</value>
                <value level="3">50</value>
                <value level="4">55</value>
                <value level="5">60</value>
            </mana>
        </consume>
        <target type="ENEMY"/>
        <abnormal type="TURN_FLEE" visual="TURN_FLEE">
            <level initial="1"/>
            <time initial="3"/>
            <chance initial="80"/>
        </abnormal>
        <effects>
            <effect name="BlockControl"/>
            <effect name="Fear"/>
        </effects>
    </skill>
</list>
